{
  "name": "n8n-telemetry-sdk",
  "version": "1.0.0",
  "private": true,
  "description": "Telemetry and Evaluation SDK for n8n with workflow and node lifecycle tracking",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w packages/telemetry-core",
    "build:node": "npm run build -w packages/n8n-node-telemetry",
    "build:extension": "npm run build -w packages/n8n-extension-telemetry",
    "clean": "rm -rf packages/*/dist",
    "view": "node scripts/view-events.js",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
