{
  "name": "n8n-nodes-telemetry",
  "version": "1.0.0",
  "description": "n8n community nodes for telemetry and workflow evaluation",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node",
    "telemetry",
    "observability",
    "monitoring",
    "evaluation"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelemetryApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelemetryConfig/TelemetryConfig.node.js",
      "dist/nodes/TelemetryEmit/TelemetryEmit.node.js"
    ]
  },
  "dependencies": {
    "@n8n-telemetry/core": "1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
